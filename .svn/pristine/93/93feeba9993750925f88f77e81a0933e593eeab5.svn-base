                                    <s:actionerror theme="jquery"/>
                                    <s:actionmessage theme="jquery"/>

                                </s:div>
                                <table border="0" cellspacing="5">

                                    <tr> <td></td><td></td> </tr> 
                                    <tr> <td></td><td></td></tr>

                                    <tr><td>User Name </td><td><s:textfield name="username" id="cusername" disabled="true"/></td>
