                                            <sj:submit 
                                                button="true"
                                                href="%{pwreseturl}"
                                                value="Accept"
                                                targets="changepassdivmsg"
                                                />      
                                            <sj:submit button="true" value="Reset" onClick="resetAllChagePass()"/>     
                                            <sj:a href="#" name="back" button="true" onclick="backToMain()">Back</sj:a>
