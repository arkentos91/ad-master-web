package com.epic.bmlalipay.bean.controlpanel.acquirer;

/**
 *
 * @author sivaganesan_t
 */
public class MerchantMgtAssignBean {
    private String currencyCode;
    private String currencyDes;
    private String flatPercentage;
    private String flatPercentageDes;
    private String amount;

    public String getCurrencyCode() {
        return currencyCode;
    }

    public void setCurrencyCode(String currencyCode) {
        this.currencyCode = currencyCode;
    }

    public String getCurrencyDes() {
        return currencyDes;
    }

    public void setCurrencyDes(String currencyDes) {
        this.currencyDes = currencyDes;
    }

    public String getFlatPercentage() {
        return flatPercentage;
    }

    public void setFlatPercentage(String flatPercentage) {
        this.flatPercentage = flatPercentage;
    }

    public String getFlatPercentageDes() {
        return flatPercentageDes;
    }

    public void setFlatPercentageDes(String flatPercentageDes) {
        this.flatPercentageDes = flatPercentageDes;
    }

    public String getAmount() {
        return amount;
    }

    public void setAmount(String amount) {
        this.amount = amount;
    }
}
