            function updateDualAuthUsers(keyval, currency) {

//                alert(keyval+' '+currency)
                $.ajax({
                    url: '${pageContext.request.contextPath}/loadTxnRiskProfile.action',
                    data: {profileCode: keyval},
                    dataType: "json",
                    type: "POST",
                    success: function(data) {
                        $('#divmsg').empty();

                        var msg = data.message;

                        if (msg) {
                            alert(data.message)
                        }
                        else {
                            $('#currency').empty();
                            $('#currency').append(
                                    $('<option></option>').val("").html("--Select Currency--")
                                    );
                            $.each(data.currencyMap, function(key, value) {
                                if (key == currency) {
                                    $('#currency').append(
                                            $('<option selected></option>').val(key).html(value)
                                            );
                                } else {
                                    $('#currency').append(
                                            $('<option></option>').val(key).html(value)
                                            );
                                }
                            });

                        }
                    },
                    error: function(data) {
                        //                        alert("Error occurd while loading.");
                        window.location = "${pageContext.request.contextPath}/LogoutUserLogin.action?";
                    }
                });
            }


            function removeDecimals(input) {
                var output = input | 0;
                return output;
            }

            function deleteRiskProfileInit(key, code, cur) {
                $('#divmsg').empty();

                $("#deletedialog").data("keyval", {alertID: key, sysCate: code, cur: cur}).dialog('open');
                $("#deletedialog").html('Are you sure you want to delete transaction risk profile : ' + code + ' ?');
                return false;
            }
            function deleteRiskProfile(keyval) {

                $.ajax({
                    url: '${pageContext.request.contextPath}/deleteTxnRiskProfile.action',
                    data: {txnType: keyval.alertID, profileCode: keyval.sysCate, currency: keyval.cur},
                    dataType: "json",
                    type: "POST",
                    success: function(data) {
                        $("#deletesuccdialog").dialog('open');
                        $("#deletesuccdialog").html(data.message);
                        $("#gridtable").trigger("reloadGrid");
                        resetFieldData();

                    },
                    error: function(data) {
                        alert("unsuccess");
                        //window.location = "${pageContext.request.contextPath}/LogoutUserLogin.action?";
                    }
                });
            }

