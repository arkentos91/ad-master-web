/*bootstarp override*/

label.control-label {
    font-size: 13px;
    font-weight: normal;
}
.form-group > label {
    font-size: 13px;
    font-weight: normal;
}
.form-control {
    background-color: rgba(255, 255, 255, 0.81)  !important;
    border-radius: 0px  !important;
    font-weight: normal  !important;
    font-size: 13px  !important;
    font-family: Raleway  !important;
    border: 1px solid #cccccc  !important;
}
.form-control:focus {
    border-color: #66afe9  !important;
    outline: 0  !important;
    -webkit-box-shadow: inset 0 0px 0px rgba(0, 0, 0, .075), 0 0 0px rgba(102, 175, 233, 0.6)  !important;
    box-shadow: inset 0 0px 0px rgba(0, 0, 0, .075), 0 0 0px rgba(102, 175, 233, 0.6)  !important;
}
.form-control[disabled],
.form-control[readonly],
fieldset[disabled] .form-control {
    background-color: rgba(61, 67, 68, 0.57)  !important;
    opacity: 1  !important;
    cursor: not-allowed  !important;
}
table a:not(.btn),
.table a:not(.btn) {
    text-decoration: none  !important;
}
.hasDatepicker[readonly] {
    background-color: rgba(255, 255, 255, 0.99)  !important;
    cursor: pointer  !important;
}
label.control-label > span {
    font-family: cursive  !important;
}


